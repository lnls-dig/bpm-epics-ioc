# Interlock control

record(bo,"$(P)$(R)IntlkEn-Sel"){
    field(DTYP, "asynUInt32Digital")
    field(DESC, "Set interlock enable")
    field(PINI, "YES")
    field(SCAN, "Passive")
    field(ZNAM, "Off")
    field(ONAM, "On")
    field(OUT,  "@asynMask($(PORT),$(ADDR),0x1,$(TIMEOUT))INTLK_EN")
}

