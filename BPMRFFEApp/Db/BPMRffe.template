record(ao,"$(P)$(R)RFFEAtt"){
    field(DTYP, "asynFloat64")
    field(DESC,"set RFFE attenuator")
    field(PINI,"YES")
    field(SCAN,"Passive")
    field(OUT,"@asyn($(PORT),$(ADDR),$(TIMEOUT))RFFE_ATT")
    field(EGU, "dB")
}

